{
    "manifest_version": 2,
    "name": "Bellarun",
    "version": "1.0",
    "description": "ez run",
    "icons": {
        "48": "icons/belarun48.png",
        "96": "icons/belarun96.png"
    },
    "browser_action": {
        "browser_style": true,
        "default_popup": "popup.html"
    },
    "content_scripts": [
        {
            "matches": [
                "https://codeforces.com/*"
            ],
            "js": [
                "borderify.js"
            ]
        }
    ]
}